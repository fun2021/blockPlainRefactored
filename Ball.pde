class Ball {
    float ball_x, ball_y, ball_dx, ball_dy, width, height;

    Ball(float ball_x, float ball_y, float ball_dx, float ball_dy, float width, float height) {
        this.ball_x = ball_x;
        this.ball_y = ball_y;
        this.ball_dx = ball_dx;
        this.ball_dy = ball_dy;
        this.width = width;
        this.height = height;
    }


}